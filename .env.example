# ============================================
# Supabase PostgreSQL Connection
# ============================================
# Get these credentials from: https://app.supabase.com/project/_/settings/database

POSTGRES_HOST=db.xxxxxxxxxxxxxx.supabase.co
POSTGRES_PORT=5432
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_supabase_password

# SSL Mode (required for Supabase)
POSTGRES_SSL_MODE=require

# ============================================
# JDBC URL (Auto-constructed for Flink)
# ============================================
# Format: jdbc:postgresql://HOST:PORT/DATABASE?sslmode=require
JDBC_URL=jdbc:postgresql://db.xxxxxxxxxxxxxx.supabase.co:5432/postgres?sslmode=require
